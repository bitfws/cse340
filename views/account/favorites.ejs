<% if (messages && messages.notice) { %>
  <div class="flash-message">
    <%- messages() %>
  </div>
<% } %>

<h1><%= title %></h1>

<% if (grid && grid.length > 0) { %>
  <%- grid %>
<% } else { %>
  <p class="notice">You have no favorite vehicles yet.</p>
<% } %>

<p class="login-link">
  <a href="/inv/">Back to Inventory</a>
</p>
