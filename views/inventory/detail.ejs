<% if (title) { %>
<h1><%= title %></h1>
<% } %>

<%- vehicleHTML %>

<% if (loggedin && accountData && accountData.account_type === 'Client') { %>
  <form action="/favorites/add" method="post">
    <input type="hidden" name="inv_id" value="<%= vehicle.inv_id %>">
    <button type="submit" class='btn'>Add to Favorites</button>
  </form>
<% } %>
